{
  "name": "arduino",
  "group": "embedded",
  "help": "generate the code for the arduino library manager",
  "generate_folders": [],
  "extra_data": {
    "use_alternate_names": true,
    "math_provider": "Arduino.h"
  },
  "per_unit_templates": [
    {
      "filename": "unit.cpp.template",
      "output_location": "src/units",
      "output_pattern": "{unit[name]}.cpp"
    }
  ],
  "clang-format": {
    "file_patters": [
      "src/unit_system.hpp",
      "src/units/*.cpp"
    ]
  }
}
