{
  "name": "python",
  "group": "python",
  "help": "generate the code for pip-py",
  "extra_install_files": [
    {
      "file": "generator_code/prefixes.py",
      "destination": "unit_system"
    }
  ],
  "test_commands": [
    {
      "command":["python", "-m", "unittest", "tests/tests.py"],
      "environment": {
        "PYTHONIOENCODING": "utf-8",
        "PYTHONLEGACYWINDOWSSTDIO": "utf-8",
        "PYTHONUTF8": "1"
      }
    }
  ]
}
