{
  "name": "python",
  "group": "python",
  "help": "generate the code for pip-py",
  "data_override": {
    "literals" : {
      "time": {
        "attosecond": {
          "code_literal": "as_"
        }
      }
    }
  },
  "formatter": {
    "name": "yapf",
    "args": ["-i"],
    "file_patters": [
      "tests/tests.py",
      "unit_system.py"
    ]
  },
  "test_commands": [
    {
      "command":["python", "-m", "unittest", "tests/tests.py"],
      "environment": {
        "PYTHONIOENCODING": "utf-8",
        "PYTHONLEGACYWINDOWSSTDIO": "utf-8",
        "PYTHONUTF8": "1"
      }
    }
  ]
}
