{
  "name": "arduino",
  "group": "embedded",
  "help": "generate the code for the arduino library manager",
  "generate_folders": [],
  "extra_data": {
    "use_alternate_names": true
  },
  "per_unit_templates": [
    {
      "filename": "unit.cpp.template",
      "output_location": "src",
      "output_pattern": "{unit[name]}.cpp"
    },
    {
      "filename": "unit.hpp.template",
      "output_location": "src/unit_system",
      "output_pattern": "{unit[name]}.hpp"
    }
  ]
}
